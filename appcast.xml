	<title>BigPond Usage Meter</title>
    {% for post in site.posts %}
    <item>
		<title>{{ post.title | xml_escape }}</title>
				<link href="http://aantthony.github.com/BigPond-Usage-Meter{{ post.url }}"/>
				<sparkle:releaseNotesLink>http://aantthony.github.com/BigPond-Usage-Meter{{ post.url }}</sparkle:releaseNotesLink>
